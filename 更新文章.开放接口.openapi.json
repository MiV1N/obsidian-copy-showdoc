  "paths": {
    "/server/index.php": {
      "post": {
        "summary": "更新文章",
        "deprecated": false,
        "description": "",
        "tags": [],
        "parameters": [
          {
            "name": "s",
            "in": "query",
            "description": "",
            "required": false,
            "example": "/api/item/updateByApi",
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "api_key": {
                    "description": "api_key，认证凭证。登录showdoc，进入具体项目后，点击项目设置-\"开放API\"便可看到",
                    "type": "string"
                  },
                  "api_token": {
                    "type": "string"
                  },
                  "cat_name": {
                    "description": "可选参数。当页面文档处于目录下时，请传递目录名。当目录名不存在时，showdoc会自动创建此目录。需要创建多层目录的时候请用斜杆隔开，例如 \"一层/二层/三层\"",
                    "type": "string"
                  },
                  "page_title": {
                    "description": "页面标题。请保证其唯一。（或者，当页面处于目录下时，请保证页面标题在该目录下唯一）。当页面标题不存在时，showdoc将会创建此页面。当页面标题存在时，将用page_content更新其内容",
                    "type": "string"
                  },
                  "page_content": {
                    "description": "页面内容，可传递markdown格式的文本或者html源码",
                    "type": "string"
                  },
                  "s_number": {
                    "description": "可选，页面序号。默认是99。数字越小，该页面越靠前",
                    "type": "integer"
                  }
                },
                "required": [
                  "api_key",
                  "api_token",
                  "cat_name",
                  "page_title",
                  "page_content",
                  "s_number"
                ]
              },
              "example": "{\r\n    \"api_key\": \"api_key_placeholder\", \r\n    \"api_token\": \"api_token_placeholder\",\r\n    \"cat_name\": \"示例目录\", \r\n    \"page_title\": \"示例页面标题\", \r\n    \"page_content\": \"# 示例内容\\n这是示例内容\", \r\n    \"s_number\": 99 \r\n}"
            }
          }
        },
        "responses": {
          "200": {
